# =============================================================================
# IronHive LLM Provider Keys
# =============================================================================
# Copy this file to .env and fill in your actual values.
# Do NOT commit .env file to version control.
# =============================================================================

# -----------------------------------------------------------------------------
# OpenAI
# https://platform.openai.com/api-keys
# Models: gpt-4o-mini ($0.15/$0.60), gpt-4o ($2.50/$10), gpt-5-nano ($0.05/$0.40)
# -----------------------------------------------------------------------------
OPENAI_API_KEY=sk-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
OPENAI_ORGANIZATION=
OPENAI_PROJECT=
OPENAI_MODEL=gpt-4o-mini

# -----------------------------------------------------------------------------
# Azure OpenAI
# https://portal.azure.com/
# -----------------------------------------------------------------------------
AZURE_OPENAI_API_KEY=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
AZURE_OPENAI_API_VERSION=2024-02-15-preview
AZURE_OPENAI_DEPLOYMENT=gpt-4o-mini

# -----------------------------------------------------------------------------
# Anthropic
# https://console.anthropic.com/settings/keys
# Models: claude-haiku-4-5 ($1/$5), claude-sonnet-4-5 ($3/$15), claude-opus-4-5 ($5/$25)
# Legacy: claude-3-5-haiku-latest ($0.25/$1.25)
# -----------------------------------------------------------------------------
ANTHROPIC_API_KEY=sk-ant-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
ANTHROPIC_MODEL=claude-3-5-haiku-latest

# -----------------------------------------------------------------------------
# Google AI (Gemini)
# https://aistudio.google.com/app/apikey
# Models: gemini-2.5-flash-lite ($0.10/$0.40), gemini-3-flash ($0.50/$3)
# -----------------------------------------------------------------------------
GOOGLE_API_KEY=AIzaxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
GOOGLE_MODEL=gemini-2.0-flash

# -----------------------------------------------------------------------------
# xAI (Grok)
# https://console.x.ai/
# Models: grok-3-mini (경제적), grok-3, grok-4-1-fast-non-reasoning
# Leave empty to auto-select from available models
# -----------------------------------------------------------------------------
XAI_API_KEY=xai-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
XAI_MODEL=grok-4-1-fast-reasoning

# -----------------------------------------------------------------------------
# Ollama (Local)
# https://ollama.ai/
# Models: llama3.2, qwen2.5, gemma2, phi3, etc.
# -----------------------------------------------------------------------------
OLLAMA_BASE_URL=http://localhost:11434/api/
OLLAMA_MODEL=

# -----------------------------------------------------------------------------
# LM Studio (Local)
# https://lmstudio.ai/
# -----------------------------------------------------------------------------
LMSTUDIO_API_KEY=lm-studio
LMSTUDIO_BASE_URL=http://localhost:1234/v1/
LMSTUDIO_MODEL=

# -----------------------------------------------------------------------------
# GPUStack
# https://github.com/gpustack/gpustack
# -----------------------------------------------------------------------------
GPUSTACK_API_KEY=gpustack-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
GPUSTACK_BASE_URL=http://localhost:80/v1-openai/
GPUSTACK_MODEL=
