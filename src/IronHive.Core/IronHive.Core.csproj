<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	<!-- Package Settings -->
	<Authors>Iyulab</Authors>
	<Description>ironhive core</Description>
	<PackageVersion>1.0.0</PackageVersion>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
	<PackageProjectUrl>https://github.com/iyulab-rnd/ironhive</PackageProjectUrl>
	<PackageTags>Iyulab;AI;LLM;RAG;Agent</PackageTags>
	
	<!-- Project Settings -->
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DocumentFormat.OpenXml" Version="3.3.0" />
    <PackageReference Include="HtmlAgilityPack" Version="1.12.3" />
    <PackageReference Include="MessagePack" Version="3.1.4" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc.Core" Version="2.3.0" />
    <PackageReference Include="Microsoft.Data.Sqlite" Version="9.0.9" />
    <PackageReference Include="PdfPig" Version="0.1.11" />
    <PackageReference Include="Scriban" Version="6.4.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\IronHive.Abstractions\IronHive.Abstractions.csproj" />
  </ItemGroup>

</Project>
